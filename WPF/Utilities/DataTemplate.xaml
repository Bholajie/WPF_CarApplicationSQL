<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:WPF.View"
                    xmlns:vm="clr-namespace:WPF.ViewModel">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginVM}">
        <view:Login/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SignUpVM}">
        <view:SignUp/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Product/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TransactionVM}">
        <view:Transaction />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NavigationVM}">
        <view:Navigation/>
    </DataTemplate>

</ResourceDictionary>